{
    "name": "qml-debug",
    "displayName": "QML Debug",
    "description": "Visual Studio Code debugging support for Qt QML language",
    "version": "0.0.1",
    "icon": "qml-debug.png",
    "author": {
        "name": "Y. Orçun Gökbulut",
        "email": "orcun.gokbulut@gmail.com",
        "url": "https://www.github.com/orcun-gokbulut"
    },
    "homepage": "https://github.com/orcun-gokbulut/qml-debug",
    "bugs": {
        "url": "https://github.com/orcun-gokbulut/qml-debug/issues",
        "email": "orcun.gokbulut@gmail.com"
    },
    "repository": {
        "type": "git",
        "url": "https://www.github.com/orcun-gokbulut/qml-debug.git"
    },
    "license": "GPL-3.0",
    "categories": [
        "Debuggers"
    ],
    "keywords": [
        "QML",
        "Qt",
        "Debugger"
    ],
    "engines": {
        "vscode": "^1.54.0"
    },
    "main": "out/extension.js",
    "activationEvents": [
        "onCommand:qml-debug.version",
        "onCommand:qml-debug.copyright"
    ],
    "contributes": {
        "commands": [
            {
                "command": "qml-debug.version",
                "title": "QML Debug: Version Information"
            },
            {
                "command": "qml-debug.copyright",
                "title": "QML Debug: Copyright Information"
            }
        ]
    },
    "_moduleAliases": {
        "@qml-debug": "./src",
        "@qml-debug-root": "."
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "bootstrap": "npm install --production",
        "bootstrap-dev": "npm install",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "test": "node ./out/test/runTest.js",
        "lint": "eslint ./src --ext ts",
        "compile": "tsc",
        "clean": "rm -rfv ./out/* ./dist/*",
        "dist-clean": "rm -rfv ./package-lock.json ./node_modules ./out ./dist"
    },
    "devDependencies": {
        "@types/glob": "^7.1.4",
        "@types/mocha": "^9.0.0",
        "@types/node": "14.x",
        "@types/vscode": "^1.54.0",
        "@typescript-eslint/eslint-plugin": "^5.1.0",
        "@typescript-eslint/parser": "^5.1.0",
        "@vscode/test-electron": "^1.6.2",
        "@vscode/debugadapter-testsupport": "^1.51.0",
        "eslint": "^8.1.0",
        "glob": "^7.1.7",
        "mocha": "^9.1.3",
        "typescript": "^4.4.4"
    },
    "dependencies": {
        "buffer-hex-dump": "../buffer-hex-dump",
        "colors": "^1.4.0",
        "module-alias": "^2.2.2",
        "promise-socket": "^7.0.0",
        "source-map-support": "^0.5.21",
        "@vscode/debugadapter": "^1.51.0",
        "@vscode/debugprotocol": "^1.51.0"
    }
}
